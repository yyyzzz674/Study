<!DOCTYPE html>
<html>
  <% include head %>   
  <body>
    <div class="container">
      <ul>
        <% articles.forEach((article) => { %>
        <li>
          <a href="/articles/<%= article.id %>">
            <%= article.title %>
          </a>
        </li>
        <% }) %>
      </ul>
    </div>
  </body>
  <% include foot %>
</html>
